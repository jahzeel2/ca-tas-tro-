                                                                                    <tr>
                                                                                        <th width="5%" class="no-sort"></th>
                                                                                        <th width="95%">@Resources.Recursos.Inspector</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    @{foreach (var item in Model.Inspectores)
                                                                                        {
                                                                                            string inspector = item.Usuario.Apellido + " " + item.Usuario.Nombre;
                                                                                            long idInspector = item.InspectorID;

                                                                                            if (item.Usuario.Fecha_baja != null)
                                                                                            {
                                                                                                <tr style="background : #fd9999;">
                                                                                                    <td width="5%">@idInspector</td>
                                                                                                    <td width="95%">@inspector</td>
                                                                                                </tr>
                                                                                            }
                                                                                            else
                                                                                            {
                                                                                                <tr>
                                                                                                    <td width="5%">@idInspector</td>
                                                                                                    <td width="95%">@inspector</td>
                                                                                                </tr>
                                                                                            }

                                                                                            continue;
                                                                                        }}
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <span id="btnBuscar" class="fa fa-search fa-lg fa-2x cursor-pointer pull-right" aria-hidden="true" style="margin-bottom:7px; margin-left:7px;" />
                                                <span id="btnAdd" class="fa fa-plus-circle fa-lg fa-2x cursor-pointer pull-right" style="margin-bottom:7px;" aria-hidden="true" />
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
