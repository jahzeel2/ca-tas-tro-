@model List<ListadoDeTareas>
@using GeoSit.Data.BusinessEntities.Seguridad
<div id="reportFileName" class="hidden">ListadoTareasPorUsuario</div>
<table id="Resultados" class="table table-bordered table-striped table-responsive">
    <thead>
        <tr>
            <th>Usuario</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Terminal</th>
            <th>Función</th>
            <th>Fecha</th>
            <th data-order="desc">Cantidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (ListadoDeTareas row in Model)
        {
            <tr>
                <td>@row.Login</td>
                <td>@row.Nombre</td>
                <td>@row.Apellido</td>
                <td>@row.Ip</td>
                <td>@Html.Raw(row.nombreFuncion)</td>
                <td>@row.Fecha</td>
                <td>@row.Cantidad</td>
            </tr>
        }
    </tbody>
</table>

