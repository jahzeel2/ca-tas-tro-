@model GeoSit.Data.BusinessEntities.Certificados.INMCertificadoCatastral
<link href="~/Content/dataTables.bootstrap.css" rel="stylesheet" />
<link href="~/Scripts/bootstrap-calendar/css/calendar.css" rel="stylesheet">

<div class="modal fade theme_new" id="modal-window-informe-tramites" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-windowLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="scroll-content">
                <div class="modal-header">
                    <h3 class="modal-title">Emisión de Certificado Catastral</h3>
                    <span aria-hidden="true" class="fa fa-close fa-2x white cursor-pointer pull-right" data-dismiss="modal" aria-label="Cerrar" title="Cerrar"></span>
                </div>
                <div class="modal-body certificadocatastral-body">
                    <div id="body-content" class="body-content certificadocatastral-content" style="padding-right: 50px">
                        <div class="panel-body">
                            <div class="form-horizontal">
                                <!--<div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Número</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" id="numero" value="" readonly/>
                                    </div>
                                </div>-->
                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Fecha Emisión</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="datetime-local" name="fechaemision" id="fechaemision" value="" />
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Motivo</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" name="motivo" id="motivo" value="" />
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Vigencia</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="number" name="vigencia" id="vigencia" value="" />
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Descripción</label>
                                    <div class="col-xs-8">
                                        <textarea class="form-control" type="text" name="descripcion" id="descripcion" value="" maxlength="4000" rows="2" data-bv-field="descripcion" style="height: 80px;"></textarea>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Observaciones</label>
                                    <div class="col-xs-8">
                                        <textarea class="form-control" type="text" name="observaciones" id="observaciones" value="" maxlength="4000" rows="2" data-bv-field="observaciones" style="height: 80px;"></textarea>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Mensura</label>
                                    <div class="col-xs-8">
                                        <div class="input-group">
                                            <input type="text" id="txtMensura" class="form-control" readonly />
                                            @Html.HiddenFor(model => model.MensuraId)
                                            <span class="input-group-btn">
                                                <button title="Buscar Mensura" id="mensura-search" class="btn btn-default">
                                                    <i class="fa fa-search"></i>
                                                </button>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Unidad Tributaria</label>
                                    <div class="col-xs-8">
                                        <div class="input-group">
                                            @Html.TextBoxFor(model => model.UnidadTributaria, new { @class = "form-control", @readonly = "true" })
                                            @Html.HiddenFor(model => model.UnidadTributariaId)
                                            <span class="input-group-btn">
                                                <button title="Buscar Unidad Tributaria" id="ut-search" class="btn btn-default">
                                                    <i class="fa fa-search"></i>
                                                </button>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-4 control-label text-right">Persona</label>
                                    <div class="col-xs-8">
                                        <div class="input-group">
                                            @Html.TextBoxFor(model => model.Solicitante, new { @class = "form-control", @readonly = "true" })
                                            @Html.HiddenFor(model => model.SolicitanteId)
                                            <span class="input-group-btn">
                                                <button title="Buscar Persona" id="persona-search" class="btn btn-default">
                                                    <i class="fa fa-search"></i>
                                                </button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="col-xs-4 pull-right">
                    <span id="aceptar-informe-tramites" aria-hidden="true" aria-controls="button" data-placement="right" title="Aceptar" data-original-title="Aceptar" class="fa fa-check-circle light-blue fa-3x cursor-pointer"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal Información-->
<div class="modal fade theme_new mensaje" id="ModalInfoInspector" tabindex="-3" role="dialog" aria-labelledby="TituloInfo" aria-hidden="true" data-backdrop="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title" id="TituloInfoInspector">Titulo</h3>
                <span aria-hidden="true" class="fa fa-close fa-2x white cursor-pointer pull-right" data-dismiss="modal" aria-label="Cerrar" title="Cerrar"></span>
            </div>
            <div class="modal-body">
                <div id="MensajeInfoInspector" class="alert alert-info alert-dismissible" role="alert">
                    <p id="DescripcionInfoInspector">Descripcion de la informacion</p>
                </div>
            </div>
            <div class="modal-footer">
                <div class="col-xs-4 pull-right">
                    <span id="btnAceptarAlert" aria-hidden="true" data-dismiss="modal" data-placement="right" title="Aceptar" data-original-title="Aceptar" class="fa fa-check-circle light-blue fa-3x cursor-pointer"></span>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="buscador-container"></div>
<div id="personas-externo-container"></div>

<script src="~/Scripts/jquery.dataTables.min.js"></script>
<script src="~/Scripts/dataTables.bootstrap.js"></script>
<script src="~/Scripts/select2.min.js"></script>
<script src="~/Scripts/fileinput.es.js"></script>
<script src="~/Scripts/jquery.form.js"></script>
<script src="~/Scripts/formValidation.min.js"></script>
<script src="~/Scripts/bootstrapValidation.min.js"></script>
<script src="~/Scripts/jquery.inputmask.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.inputmask.regex.extensions.js" type="text/javascript"></script>

<script type="text/javascript" src="~/Scripts/ExpedienteObra/CertificadoCatastral.js"></script>