@model GeoSit.Client.Web.Models.Search.SearchResultGroup
@foreach(dynamic cuenta  in Model.Items)
{
    <div class='srow cuentas'>
        <div class="controls">
            <span class="glyphicon glyphicon-plus-sign pull-right tooltips" data-placement="right" title="Más información" data-toggle="modal" data-target="#modal-window"></span>
            <span class="glyphicon glyphicon-pushpin  pull-right tooltips" data-placement="right" title="Pin"></span>
        </div>
        <div class="rcheck cuentas">
            <input title="Seleccionar" type="checkbox" class="rcheck" />
        </div>
        <p class='text-primary'>
            @Html.Raw(cuenta.nomenclatura)
        </p>
        <p>Domicilio:&nbsp;@Html.Raw(cuenta.domicilio)</p>
        <p>Localidad:&nbsp;@Html.Raw(cuenta.localidad)</p>
        <p class='text-muted'>Departamento:&nbsp;@Html.Raw(cuenta.departamento)</p>
    </div>
}
