@model GeoSit.Client.Web.Models.MapaTematicoModel

@{
    ViewBag.Title = "Visualización";
    ViewBag.Description = "Configuración de visualización del mapa temático.";
    bool eslinea = Model.Componente.Graficos == 2;
    bool espunto = Model.Componente.Graficos == 3;

    string tooltipAyuda =
    @"- Uniforme: Configura intervalos de igual magnitud para cada rango.
- Cuantiles: Genera los rangos dividiendo la cantidad de resultados por la cantidad de rangos configurados por el usuario.
- Valores individuales: Separa los resultados de acuerdo a atributos preestablecidos.";
}


<link href="~/Content/pick-a-color-1.2.3.min.css" rel="stylesheet" />
<link href="~/Content/bootstrap-fileinput/css/fileinput.min.css" rel="stylesheet">
<link href="~/Content/mapastematicos.css?v=@(DateTime.Now.Ticks.ToString())" rel="stylesheet" />

<div class="modal fade theme_new mapas-tematicos visualizacion" id="myModalMT" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog modal-xlg">
        <div class="modal-content">
            <div class="scroll-content">
